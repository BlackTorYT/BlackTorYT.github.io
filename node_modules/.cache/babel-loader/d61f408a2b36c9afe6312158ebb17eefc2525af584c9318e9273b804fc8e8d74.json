{"ast":null,"code":"export default {\n  props: {\n    channels: {\n      type: Array,\n      required: true\n    },\n    isEdit: {\n      type: Boolean,\n      default: false\n    },\n    channel: {\n      type: Object,\n      default: () => ({\n        title: '',\n        code: '',\n        mailing: 'Telegram Bot',\n        disc: '',\n        json: '{ \"telegram_token\":\"\"}',\n        status: true\n      })\n    }\n  },\n  data() {\n    return {\n      errors: {\n        title: '',\n        code: ''\n      }\n    };\n  },\n  methods: {\n    getNextId() {\n      if (this.channels.length === 0) {\n        return 1;\n      }\n      const maxId = Math.max(...this.channels.map(channel => channel.id));\n      return maxId + 1;\n    },\n    createChannel() {\n      this.clearErrors();\n      if (!this.channel.title) {\n        this.errors.title = 'Название обязательно для заполнения';\n      }\n      if (!this.channel.code) {\n        this.errors.code = 'Уникальный код обязательно для заполнения';\n      }\n      if (this.errors.title || this.errors.code) {\n        return;\n      }\n      const newChannelId = this.getNextId();\n      const newChannel = {\n        ...this.channel,\n        id: newChannelId\n      };\n      this.$emit('create', newChannel);\n      this.resetForm();\n    },\n    clearErrors() {\n      this.errors = {\n        title: '',\n        code: ''\n      };\n    },\n    resetForm() {\n      this.channel = {\n        title: '',\n        code: '',\n        mailing: 'Telegram Bot',\n        disc: '',\n        json: '{ \"telegram_token\":\"\"}',\n        status: true\n      };\n    }\n  },\n  watch: {\n    isEdit(newVal) {\n      if (!newVal) {\n        this.resetForm();\n      }\n    },\n    channel: {\n      deep: true,\n      handler() {\n        this.clearErrors();\n      }\n    }\n  }\n};","map":{"version":3,"names":["props","channels","type","Array","required","isEdit","Boolean","default","channel","Object","title","code","mailing","disc","json","status","data","errors","methods","getNextId","length","maxId","Math","max","map","id","createChannel","clearErrors","newChannelId","newChannel","$emit","resetForm","watch","newVal","deep","handler"],"sources":["F:\\Vue Notifications\\notifications_practice\\src\\components\\ChannelForm.vue"],"sourcesContent":["<template>\n    <form class=\"wrapper\" @submit.prevent=\"createChannel\" style=\"background-color: white;\">\n      <div class=\"wrapper__head\">\n        <h1>{{ isEdit ? 'Редактирование' : 'Создание' }} канала отправки</h1>\n        <span>Панель управления / Каналы отправки / {{ isEdit ? 'Редактирование' : 'Создание' }} канала отправки </span>\n      </div>\n      <div class=\"name\">\n        Название\n        <my-input type=\"text\" class=\"input\" v-model=\"localChannel.title\" />\n        <span v-if=\"errors.title\" class=\"error\">{{ errors.title }}</span>\n      </div>\n      <div class=\"uniqueCode\">\n        Уникальный код поля\n        <my-input type=\"text\" class=\"input\" v-model=\"localChannel.code\" />\n        <span v-if=\"errors.code\" class=\"error\">{{ errors.code }}</span>\n      </div>\n      <div class=\"mailing\">\n        Обработчик\n        <select class=\"input\" v-model=\"localChannel.mailing\">\n          <option value=\"Telegram Bot\">Telegram Bot</option>\n          <option value=\"SMS\">SMS</option>\n        </select>\n      </div>\n      <div class=\"disc\">\n        Описание\n        <my-input type=\"text\" class=\"input\" v-model=\"localChannel.disc\" />\n      </div>\n      <div class=\"json\">\n        Настройки JSON\n        <my-input type=\"text\" class=\"input\" v-model=\"localChannel.json\" />\n      </div>\n      <div class=\"active\">\n        Канал активен?\n        <select class=\"input\" v-model=\"localChannel.status\">\n          <option :value=\"true\">Да</option>\n          <option :value=\"false\">Нет</option>\n        </select>\n      </div>\n      <button type=\"submit\" class=\"save__btn\">Сохранить</button>\n    </form>\n  </template>\n  \n  <script>\nexport default {\n  props: {\n    channels: {\n      type: Array,\n      required: true\n    },\n    isEdit: {\n      type: Boolean,\n      default: false\n    },\n    channel: {\n      type: Object,\n      default: () => ({\n        title: '',\n        code: '',\n        mailing: 'Telegram Bot',\n        disc: '',\n        json: '{ \"telegram_token\":\"\"}',\n        status: true\n      })\n    }\n  },\n  data() {\n    return {\n      errors: {\n        title: '',\n        code: ''\n      }\n    };\n  },\n  methods: {\n    getNextId() {\n      if (this.channels.length === 0) {\n        return 1;\n      }\n      const maxId = Math.max(...this.channels.map(channel => channel.id));\n      return maxId + 1;\n    },\n    createChannel() {\n      this.clearErrors();\n\n      if (!this.channel.title) {\n        this.errors.title = 'Название обязательно для заполнения';\n      }\n\n      if (!this.channel.code) {\n        this.errors.code = 'Уникальный код обязательно для заполнения';\n      }\n\n      if (this.errors.title || this.errors.code) {\n        return;\n      }\n\n      const newChannelId = this.getNextId();\n      const newChannel = { ...this.channel, id: newChannelId };\n      this.$emit('create', newChannel);\n      this.resetForm();\n    },\n    clearErrors() {\n      this.errors = {\n        title: '',\n        code: ''\n      };\n    },\n    resetForm() {\n      this.channel = {\n        title: '',\n        code: '',\n        mailing: 'Telegram Bot',\n        disc: '',\n        json: '{ \"telegram_token\":\"\"}',\n        status: true\n      };\n    }\n  },\n  watch: {\n    isEdit(newVal) {\n      if (!newVal) {\n        this.resetForm();\n      }\n    },\n    channel: {\n      deep: true,\n      handler() {\n        this.clearErrors();\n      }\n    }\n  }\n};\n</script>\n  \n\n<style scoped>\n.save__btn {\n  display: block;\n  width: 100%;\n  background-color: #3386f3;\n  color: white;\n  border: none;\n  padding: 10px;\n  text-align: center;\n  text-decoration: none;\n  font-size: 16px;\n  cursor: pointer;\n  margin-top: 10px;\n}\n\n.input {\n  margin-bottom: 10px;\n  padding: 8px;\n}\n\n.error {\n  color: red;\n  font-size: 14px;\n}\n</style>\n"],"mappings":"AA2CA,eAAe;EACbA,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;IACDC,MAAM,EAAE;MACNH,IAAI,EAAEI,OAAO;MACbC,OAAO,EAAE;IACX,CAAC;IACDC,OAAO,EAAE;MACPN,IAAI,EAAEO,MAAM;MACZF,OAAO,EAAEA,CAAA,MAAO;QACdG,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,cAAc;QACvBC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,wBAAwB;QAC9BC,MAAM,EAAE;MACV,CAAC;IACH;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNP,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDO,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,IAAI,CAAClB,QAAQ,CAACmB,MAAK,KAAM,CAAC,EAAE;QAC9B,OAAO,CAAC;MACV;MACA,MAAMC,KAAI,GAAIC,IAAI,CAACC,GAAG,CAAC,GAAG,IAAI,CAACtB,QAAQ,CAACuB,GAAG,CAAChB,OAAM,IAAKA,OAAO,CAACiB,EAAE,CAAC,CAAC;MACnE,OAAOJ,KAAI,GAAI,CAAC;IAClB,CAAC;IACDK,aAAaA,CAAA,EAAG;MACd,IAAI,CAACC,WAAW,CAAC,CAAC;MAElB,IAAI,CAAC,IAAI,CAACnB,OAAO,CAACE,KAAK,EAAE;QACvB,IAAI,CAACO,MAAM,CAACP,KAAI,GAAI,qCAAqC;MAC3D;MAEA,IAAI,CAAC,IAAI,CAACF,OAAO,CAACG,IAAI,EAAE;QACtB,IAAI,CAACM,MAAM,CAACN,IAAG,GAAI,2CAA2C;MAChE;MAEA,IAAI,IAAI,CAACM,MAAM,CAACP,KAAI,IAAK,IAAI,CAACO,MAAM,CAACN,IAAI,EAAE;QACzC;MACF;MAEA,MAAMiB,YAAW,GAAI,IAAI,CAACT,SAAS,CAAC,CAAC;MACrC,MAAMU,UAAS,GAAI;QAAE,GAAG,IAAI,CAACrB,OAAO;QAAEiB,EAAE,EAAEG;MAAa,CAAC;MACxD,IAAI,CAACE,KAAK,CAAC,QAAQ,EAAED,UAAU,CAAC;MAChC,IAAI,CAACE,SAAS,CAAC,CAAC;IAClB,CAAC;IACDJ,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACV,MAAK,GAAI;QACZP,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE;MACR,CAAC;IACH,CAAC;IACDoB,SAASA,CAAA,EAAG;MACV,IAAI,CAACvB,OAAM,GAAI;QACbE,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,cAAc;QACvBC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,wBAAwB;QAC9BC,MAAM,EAAE;MACV,CAAC;IACH;EACF,CAAC;EACDiB,KAAK,EAAE;IACL3B,MAAMA,CAAC4B,MAAM,EAAE;MACb,IAAI,CAACA,MAAM,EAAE;QACX,IAAI,CAACF,SAAS,CAAC,CAAC;MAClB;IACF,CAAC;IACDvB,OAA